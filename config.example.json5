//      也可以使用 JSON 格式的 config.json
{
	"botToken": "1234567890:abcdef", // 機器人 TOKEN
	"activeGroupMap": [  // 機器人 TOKEN
		{
			"publicGroup": -123456,
			"privateGroup": -123456
		}
	],
	// 紀錄等級，有 debug、info、warn、error 幾種等級，推薦使用 info
	"logLevel": "info",
	// 紀錄檔檔名，如留空則只向螢幕輸出
	"logFile": "app.log"
}
